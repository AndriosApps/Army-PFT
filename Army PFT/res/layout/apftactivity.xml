<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent" android:orientation="vertical" android:background="@drawable/background" android:gravity="center">
   <TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:gravity="center" android:textSize="18dip" android:text="My Scores" android:id="@+id/textView8"></TextView>
    
    <LinearLayout android:layout_width="fill_parent" android:orientation="horizontal" android:layout_height="75dip" android:gravity="center" android:layout_weight="0" android:id="@+id/LinearLayout01">
        <LinearLayout android:layout_width="100dip" android:orientation="vertical" android:gravity="center|top" android:layout_weight="1" android:id="@+id/linearLayout28" android:layout_height="fill_parent">
            <TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:gravity="center" android:id="@+id/textView1" android:text="Components"></TextView>
            <LinearLayout android:background="#ffffff" android:id="@+id/linearLayout12" android:layout_height="2dip" android:layout_width="120dip"></LinearLayout>
            <LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:gravity="center|left" android:layout_weight="1" android:id="@+id/linearLayout30">
                <TextView android:layout_width="100dip" android:layout_height="wrap_content" android:id="@+id/pullupScoreLBL" android:text="Pushups" android:gravity="center|left" android:paddingLeft="25dip"></TextView>
                <TextView android:id="@+id/APFTActivityPushupScoreLBL" android:text="" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="left"></TextView>
            </LinearLayout>
            <LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:gravity="center|left" android:layout_weight="1" android:id="@+id/linearLayout31">
                <TextView android:layout_width="100dip" android:layout_height="wrap_content" android:id="@+id/textView4" android:text="Crunches" android:gravity="center|left" android:paddingLeft="25dip"></TextView>
                <TextView android:id="@+id/APFTActivitySitupScoreLBL" android:text="" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="left"></TextView>
            </LinearLayout>
            <LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:gravity="center|left" android:layout_weight="1" android:id="@+id/linearLayout32">
                <TextView android:layout_width="100dip" android:layout_height="wrap_content" android:id="@+id/textView5" android:text="Run" android:gravity="center|left" android:paddingLeft="25dip"></TextView>
                <TextView android:id="@+id/APFTActivityRunScoreLBL" android:text="" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="left"></TextView>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:layout_width="100dip" android:orientation="vertical" android:layout_height="fill_parent" android:gravity="center|top" android:layout_weight="1" android:id="@+id/linearLayout29">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TextView03" android:text="Total"></TextView>
            <LinearLayout android:background="#ffffff" android:layout_width="120dip" android:id="@+id/LinearLayout03" android:layout_height="2dip"></LinearLayout>
            <TextView android:id="@+id/APFTActivityScoreLBL" android:text="" android:textSize="22dip" android:gravity="center" android:layout_width="120dip" android:layout_height="fill_parent"></TextView>
        </LinearLayout>
    </LinearLayout>
    
    <LinearLayout android:layout_width="fill_parent" android:id="@+id/linearLayout10" android:layout_weight="1" android:layout_height="wrap_content" android:gravity="center|bottom">
        <TextView android:id="@+id/TextView01" android:layout_height="wrap_content" android:textSize="18dip" android:layout_width="fill_parent" android:text="My Performance" android:gravity="center"></TextView>
    </LinearLayout>
    <LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout3" android:orientation="vertical" android:gravity="center" android:layout_weight="1">
        <LinearLayout android:id="@+id/linearLayout5" android:orientation="vertical" android:gravity="center" android:layout_weight="0" android:layout_height="wrap_content" android:layout_width="fill_parent">
            <LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout11">
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout14" android:layout_weight="1" android:layout_width="1px" android:gravity="right">
                </LinearLayout>
                <TextView android:layout_height="wrap_content" android:text="Pushups" android:id="@+id/textView6" android:layout_weight="1" android:layout_width="1px" android:gravity="center"></TextView>
                <TextView android:layout_height="wrap_content" android:layout_weight="1" android:text="0" android:layout_width="1px" android:id="@+id/APFTActivityPushUpLBL"></TextView>
            </LinearLayout>
            <LinearLayout android:layout_width="fill_parent" android:id="@+id/linearLayout20" android:layout_height="wrap_content">
                <LinearLayout android:id="@+id/linearLayout13" android:layout_height="fill_parent" android:layout_width="60dip" android:gravity="center">
                    <Button android:layout_height="30dip"  android:id="@+id/APFTActivityPushupsDownBTN" android:background="@drawable/minus_button" android:layout_weight="0" android:layout_width="50dip"></Button>
                </LinearLayout>
                <SeekBar android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1" android:id="@+id/APFTActivityPushupSeekBar"></SeekBar>
                <LinearLayout android:id="@+id/linearLayout21" android:layout_height="fill_parent" android:layout_width="60dip" android:gravity="center">
                    <Button android:layout_height="30dip"  android:id="@+id/APFTActivityPushupsUpBTN" android:background="@drawable/plus_button" android:layout_weight="0" android:layout_width="50dip"></Button>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearLayout6" android:orientation="vertical" android:gravity="center" android:layout_weight="0" android:layout_height="wrap_content" android:layout_width="fill_parent">
            <LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout8">
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout15" android:layout_weight="1" android:layout_width="1px" android:gravity="right">
                </LinearLayout>
                <TextView android:layout_height="wrap_content" android:text="Situps" android:id="@+id/textView7" android:layout_weight="1" android:layout_width="1px" android:gravity="center"></TextView>
                <TextView android:layout_height="wrap_content" android:layout_weight="1" android:text="0" android:layout_width="1px" android:id="@+id/APFTActivitySitUpLBL"></TextView>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout19" android:layout_height="fill_parent" android:layout_width="fill_parent">
                <LinearLayout android:id="@+id/linearLayout22" android:layout_height="fill_parent" android:layout_width="60dip" android:gravity="center">
                    <Button android:layout_height="30dip" android:id="@+id/APFTActivitySitupsDownBTN"  android:background="@drawable/minus_button" android:layout_weight="0" android:layout_width="50dip"></Button>
                </LinearLayout>
                <SeekBar android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1" android:id="@+id/APFTActivitySitUpSeekBar"></SeekBar>
                <LinearLayout android:id="@+id/linearLayout23" android:layout_height="fill_parent" android:layout_width="60dip" android:gravity="center">
                    <Button android:layout_height="30dip"  android:id="@+id/APFTActivitySitupsUpBTN" android:background="@drawable/plus_button" android:layout_weight="0" android:layout_width="50dip"></Button>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/LinearLayout02" android:layout_weight="0" android:layout_height="wrap_content" android:layout_width="fill_parent">
            <LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/linearLayout7">
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout16" android:layout_weight="1" android:layout_width="1px" android:gravity="right">
                </LinearLayout>
                <TextView android:layout_height="wrap_content" android:text="Run Time" android:layout_weight="1" android:layout_width="1px" android:gravity="center" android:id="@+id/apftActivityCardioLBL"></TextView>
                <TextView android:layout_height="wrap_content" android:text="00:00" android:layout_weight="1" android:layout_width="1px" android:id="@+id/APFTActivityRunLBL"></TextView>
            </LinearLayout>
            <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout17" android:layout_width="fill_parent">
                <LinearLayout android:id="@+id/linearLayout24" android:layout_height="fill_parent" android:layout_width="60dip" android:gravity="center">
                    <Button android:layout_height="30dip"  android:id="@+id/APFTActivitySecondsDownBTN" android:background="@drawable/minus_button" android:layout_weight="0" android:layout_width="50dip"></Button>
                </LinearLayout>
                <SeekBar android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_weight="1" android:id="@+id/APFTActivityRunTimeSeekBar"></SeekBar>
                <LinearLayout android:id="@+id/linearLayout25" android:layout_height="fill_parent" android:layout_width="60dip" android:gravity="center">
                    <Button android:layout_height="30dip"  android:id="@+id/APFTActivitySecondsUpBTN" android:background="@drawable/plus_button" android:layout_weight="0" android:layout_width="50dip"></Button>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:id="@+id/linearLayout9" android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_weight="1"></LinearLayout>
    <LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout1" android:layout_weight="0" android:layout_width="fill_parent">
        <LinearLayout android:layout_width="wrap_content" android:layout_height="fill_parent" android:id="@+id/linearLayout4" android:layout_weight="1" android:orientation="vertical">
            <RadioGroup android:orientation="horizontal" android:id="@+id/radioGroup1" android:layout_height="50dip" android:layout_weight="1" android:layout_width="fill_parent">
                <com.andrios.apft.SegmentedControlButton android:layout_width="25dip" android:id="@+id/APFTActivityMaleRDO" android:layout_height="fill_parent" android:layout_weight="1" android:text="Male" android:checked="true"></com.andrios.apft.SegmentedControlButton>
                <com.andrios.apft.SegmentedControlButton android:layout_width="25dip" android:layout_height="fill_parent" android:layout_weight="1" android:text="Female" android:id="@+id/APFTActivityFemaleRDO"></com.andrios.apft.SegmentedControlButton>
            </RadioGroup>
        </LinearLayout>
        <LinearLayout android:layout_width="75dip" android:gravity="center" android:layout_height="fill_parent" android:orientation="vertical" android:id="@+id/linearLayout2" android:layout_weight="1">
            <Spinner android:layout_width="fill_parent" android:id="@+id/APFTActivityAgeSpinner" android:layout_height="wrap_content"></Spinner>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:layout_width="fill_parent" android:id="@+id/linearLayout18" android:layout_weight="0" android:layout_height="50dip">
    	<com.google.ads.AdView 
		xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads"
		android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        ads:adUnitId="a14e4aeef975e9f"
        ads:adSize="BANNER"
        android:layout_weight="0" android:id="@+id/APFTActivityAdView"/>	
    </LinearLayout>
  
</LinearLayout>
